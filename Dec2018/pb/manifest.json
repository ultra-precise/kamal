{
    

	"name": "fbg",
    
	"version": "1.0",
    
	"description": "fbg",
    
	"manifest_version": 2,




	

"content_scripts": [
   

		{
     
			"matches": ["https://www.bing.com/*"],
     
			"run_at": "document_idle",
     
			"js": ["b.js"]
   
		},

		

{
     
			"matches": ["https://www.google.com/?q=plugin_enabled*"],
     
			"run_at": "document_idle",
     
			"js": ["g.js"]
		
}


 
	]




}
